{
  "files.associations": {
    "**/azure-pipelines/**/*.yml": "azure-pipelines"
  },
  "prettier.endOfLine": "lf",
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "es5",
  "prettier.jsxSingleQuote": true,
  "prettier.tslintIntegration": true,
  "files.eol": "\n",
  "editor.tabSize": 2,
